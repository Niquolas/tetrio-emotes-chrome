{
  "name": "Emote Picker for TETR.IO",
  "description": "Adds emote autocompletion to TETR.IO",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["https://tetr.io/*"],
      "js": ["picker.js"],
      "css": ["picker.css"],
      "run_at": "document_start"
    }
  ]
}
